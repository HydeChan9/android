<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".FinForyou">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/textView162"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="commercial"
            android:textSize="34dp" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:orientation="horizontal">

        <WebView
            android:id="@+id/Webview5"
            android:layout_width="match_parent"
            android:layout_height="635dp" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="bottom"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="bottom"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/cardview_dark_background"
                android:gravity="bottom"
                android:orientation="horizontal">

                <Button
                    android:id="@+id/button215"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="5dp"
                    android:layout_marginLeft="5dp"
                    android:layout_weight="1"
                    android:cursorVisible="false"
                    android:text="home"
                    android:textSize="8sp"
                    app:backgroundTint="@color/common_google_signin_btn_tint"
                    app:cornerRadius="10dp" />

                <Button
                    android:id="@+id/button216"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="5dp"
                    android:layout_marginLeft="5dp"
                    android:layout_weight="1"
                    android:text="account"
                    android:textSize="8sp"
                    app:backgroundTint="@color/common_google_signin_btn_tint"
                    app:cornerRadius="10dp" />

                <Button
                    android:id="@+id/button217"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="5dp"
                    android:layout_marginLeft="5dp"
                    android:layout_weight="1"
                    android:text="pay"
                    android:textSize="8sp"
                    app:backgroundTint="@color/common_google_signin_btn_tint"
                    app:cornerRadius="10dp" />

                <Button
                    android:id="@+id/button218"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="5dp"
                    android:layout_marginLeft="5dp"
                    android:layout_weight="1"
                    android:text="card"
                    android:textSize="8sp"
                    app:backgroundTint="@color/common_google_signin_btn_tint"
                    app:cornerRadius="10dp" />

                <Button
                    android:id="@+id/button219"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="5dp"
                    android:layout_marginLeft="5dp"
                    android:layout_marginEnd="5dp"
                    android:layout_marginRight="5dp"
                    android:layout_weight="1"
                    android:text="comm"
                    android:textSize="8sp"
                    app:backgroundTint="@color/common_google_signin_btn_tint"
                    app:cornerRadius="10dp" />
            </LinearLayout>
        </LinearLayout>

    </LinearLayout>
</LinearLayout>